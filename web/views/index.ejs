			var ran2_pattern = Math.floor(Math.random() * 2);
			var ran3_pattern = Math.floor(Math.random() * 2);
			var ran4_pattern = Math.floor(Math.random() * 2);				
			
			
			if( ran1_pattern == 1){
				btn1.innerHTML  = words_front[ran1];
			}else{
				btn1.innerHTML  = words_back[ran1];
			}
				
			if( ran2_pattern == 1){

		words_back = []
				
		var ranNum = <%- words.length %>
		
		<% for (var i = 0; i < words.length ; i++) { %>
				words_front.push('<%- words[i].front %>');
				words_back.push('<%- words[i].back %>');
		<%}%>		
		

		
		btn_A.addEventListener('click', function handleClick() {
			

	
			<% for (var i = 0; i < words.length ; i++) { %>
			
				if( btn1.innerHTML ==  '<%= words[i].front %>'  ){ 
					var index_1 = '<%- i %>';
				}
				

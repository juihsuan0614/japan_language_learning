			}

		});				

		btn_Random.addEventListener('click', function handleClick() {
			
			var ran1 = Math.floor(Math.random() * ranNum);
			var ran2 = Math.floor(Math.random() * ranNum);
			var ran3 = Math.floor(Math.random() * ranNum);
			var ran4 = Math.floor(Math.random() * ranNum);
				
